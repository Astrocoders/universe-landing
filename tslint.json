{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-react", "tslint-config-prettier", "tslint-plugin-prettier"],
  "jsRules": {},
  "rules": {
      "prettier": [
          true,
          {
              "tabWidth": 2,
              "singleQuote": true,
              "trailingComma": "all",
              "semi": false,
              "print-width": 120,
              "write": true
          }
      ],
    "interface-name": false,
    "member-access": [false],
    "ordered-imports": [true],
    "no-var-requires": true,
    "jsx-no-lambda": false
  },
  "rulesDirectory": []
}
